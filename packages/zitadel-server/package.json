{
  "name": "@shubhamranjan/zitadel-server",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "generate": "buf generate --path ../../proto/zitadel",
    "build": "tsup --dts",
    "test": "pnpm test:unit",
    "test:watch": "pnpm test:unit:watch",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "dev": "tsup --dts --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf src/proto"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.28.1",
    "@types/jest": "^29.5.11",
    "@zitadel/tsconfig": "file: *",
    "eslint": "^8.56.0",
    "eslint-config-zitadel": "file: *",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.166.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shubhamranjan/zitadel-ts.git"
  },
  "dependencies": {
    "jose": "^5.2.0",
    "long": "^5.2.3",
    "nice-grpc": "2.1.7",
    "nice-grpc-common": "^2.0.2",
    "protobufjs": "^7.2.6"
  }
}